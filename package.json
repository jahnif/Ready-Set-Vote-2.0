{
  "name": "readysetvote",
  "version": "0.1.0",
  "description": "Ready, Set, Vote! is a tool that allows voters to get the information they need about the various candidates running for office so that they can make an informed decision.",
  "main": "server/index.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server/index.js",
    "server": "env-cmd server/config/dev.env nodemon server/index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client",
    "test": "env-cmd server/config/test.env jest --watch --detectOpenHandles --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jahnif/Ready-Set-Vote-2.0.git"
  },
  "author": "Joseph Peha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jahnif/Ready-Set-Vote-2.0/issues"
  },
  "homepage": "https://github.com/jahnif/Ready-Set-Vote-2.0#readme",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.14",
    "multer": "^1.4.1",
    "sharp": "^0.22.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "env-cmd": "^8.0.2",
    "expect": "^24.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.10",
    "supertest": "^3.4.2"
  }
}
